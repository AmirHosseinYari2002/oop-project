<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.RadialGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StartGame">
   <children>
      <Rectangle fx:id="backGround" arcHeight="5.0" arcWidth="5.0" height="400.0" stroke="BLACK" strokeType="INSIDE" width="600.0">
         <fill>
            <RadialGradient centerX="0.5" centerY="0.5" radius="0.5">
               <stops>
                  <Stop>
                     <color>
                        <Color red="0.11999999731779099" green="1.0" blue="0.3285548985004425" />
                     </color>
                  </Stop>
                  <Stop offset="1.0">
                     <color>
                        <Color red="0.24654971063137054" green="0.6526315808296204" blue="0.4022144377231598" />
                     </color>
                  </Stop>
               </stops>
            </RadialGradient>
         </fill>
      </Rectangle>
      <ImageView fx:id="userImage" fitHeight="197.0" fitWidth="214.0" layoutX="208.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../sample/pictures/username.png" />
         </image>
      </ImageView>
      <Button fx:id="signInBtn" layoutX="172.0" layoutY="231.0" mnemonicParsing="false" onAction="#signInProcess" prefHeight="58.0" prefWidth="119.0" text="Sign in">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button fx:id="signUpBtn" layoutX="319.0" layoutY="231.0" mnemonicParsing="false" onAction="#signupProcess" prefHeight="58.0" prefWidth="119.0" text="Sign Up">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
