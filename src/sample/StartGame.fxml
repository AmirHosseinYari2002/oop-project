<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.RadialGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="479.0" prefWidth="552.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StartGame">
   <children>
      <ImageView fitHeight="485.0" fitWidth="557.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pictures/bg.jpg" />
         </image>
      </ImageView>
      <Button fx:id="signInBtn" layoutX="161.0" layoutY="204.0" mnemonicParsing="false" onAction="#signInProcess" prefHeight="38.0" prefWidth="102.0" text="Sign in">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
         <textFill>
            <RadialGradient centerX="0.5088888804117838" centerY="0.5" focusDistance="0.14953274593175014" radius="0.8644859902212554">
               <stops>
                  <Stop>
                     <color>
                        <Color red="1.0" />
                     </color>
                  </Stop>
                  <Stop offset="1.0">
                     <color>
                        <Color green="0.13294784724712372" blue="1.0" />
                     </color>
                  </Stop>
               </stops>
            </RadialGradient>
         </textFill>
      </Button>
      <Button fx:id="signUpBtn" layoutX="161.0" layoutY="288.0" mnemonicParsing="false" onAction="#signupProcess" prefHeight="38.0" prefWidth="102.0" text="Sign Up">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
         <textFill>
            <RadialGradient centerX="0.5088888804117838" centerY="0.5" focusDistance="0.14953274593175014" radius="0.8644859902212554">
               <stops>
                  <Stop>
                     <color>
                        <Color red="1.0" />
                     </color>
                  </Stop>
                  <Stop offset="1.0">
                     <color>
                        <Color green="0.13294784724712372" blue="1.0" />
                     </color>
                  </Stop>
               </stops>
            </RadialGradient>
         </textFill>
      </Button>
   </children>
</AnchorPane>
